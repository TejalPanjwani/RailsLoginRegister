
<h1> login </h1>


<%= form_with scope: :users ,url: sessions_create_path,method: :get , local: true  do |f| %>
    <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
  <% end %>
    <%= f.text_field :username, placeholder: "UserName" %> <br><br>
    <%= f.password_field :password, placeholder: "Password" %> <br><br>
    <%= f.submit "login" , :name=>"login" %>
<% end %>




<%= link_to "register", singup_users_path %>